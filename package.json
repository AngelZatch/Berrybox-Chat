{
    "name": "tamarillo",
    "version": "1.9.1",
    "description": "Node server for berrybox - Handles sync, chat",
    "main": "index.js",
    "engines": {
        "node": ">=10.17.0"
    },
    "scripts": {
        "test": "export NODE_ENV=test && mocha --exit -r ts-node/register test/**/**/*.spec.ts",
        "coverage": "nyc -e '.ts' --include-all-sources --r html -r lcov -r text npm run test",
        "tsc": "./node_modules/.bin/tsc",
        "emails": "cp -r ./src/services/MailService/emails ./dist/services/MailService",
        "start": "tsc && pm2 start processes.json",
        "restart": "tsc && pm2 restart processes.json",
        "restart:prod": "pm2 restart processes.json",
        "build": "npm run tsc && npm run emails",
        "lint:check": "eslint -c .eslintrc.json --ext .ts src",
        "lint:fix": "eslint -c .eslintrc.json --ext .ts src --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TeamBerry/tamarillo.git"
    },
    "author": "AngelZatch",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/TeamBerry/tamarillo/issues"
    },
    "homepage": "https://github.com/TeamBerry/tamarillo#readme",
    "dependencies": {
        "@teamberry/muscadine": "^0.8.4",
        "array-move": "^3.0.0",
        "aws-sdk": "^2.653.0",
        "axios": "^0.19.2",
        "bcrypt": "^5.0.0",
        "body-parser": "^1.19.0",
        "bull": "^3.13.0",
        "cors": "^2.8.4",
        "dotenv": "^8.2.0",
        "email-templates": "^7.0.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.2.2",
        "lodash": "^4.17.19",
        "moment": "^2.20.1",
        "mongoose": "^5.9.7",
        "multer": "^1.4.2",
        "nodemailer": "^6.0.0",
        "socket.io": "^2.3.0"
    },
    "devDependencies": {
        "@types/bcrypt": "3.0.0",
        "@types/chai": "4.2.12",
        "@types/mocha": "8.0.2",
        "@types/mongoose": "5.7.36",
        "@types/node": "9.6.57",
        "@typescript-eslint/eslint-plugin": "3.9.1",
        "@typescript-eslint/parser": "3.9.1",
        "chai": "4.2.0",
        "chai-as-promised": "7.1.1",
        "eslint": "7.7.0",
        "migrate-mongo": "8.1.1",
        "mocha": "8.1.1",
        "nyc": "15.1.0",
        "supertest": "4.0.2",
        "ts-node": "8.10.2",
        "typescript": "3.9.7"
    }
}
